<%= form_with model: [@test, @question], local: true do |form| %>
    <%= render 'shared/errors', resource: @question %>
    <p>
      <%= form.label :body %><br>
      <%= form.text_field :body, id: :question_body %>
    </p>

    <p>
      <%= form.label :theme %><br>
      <%= form.text_field :theme, id: :question_theme %>
    </p>

    <p><%= form.submit %></p>

<% end %>