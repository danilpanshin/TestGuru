<tr align="center">
  <td><%= question.id %></td>
  <td><%= question.body%></td>
  <td><%= question.theme %></td>
  <td><%= question.test_id %></td>
  <td>  <%= link_to 'Show', question_path(question) %> </td>
  <td>  <%= link_to 'Edit', edit_question_path(question) %> </td>
  <td>
    <%= link_to 'Delete',
                question_path(question),
                method: :delete,
                data: { confirm: 'Are you sure?'} %>
  </td>

</tr>